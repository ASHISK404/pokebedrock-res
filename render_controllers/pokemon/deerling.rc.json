{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.pokemon:deerling": {
      "arrays": {
        "geometries": {
          "Array.geometryVariants": [
            "Geometry.default",
            "Geometry.summer",
            "Geometry.autumn",
            "Geometry.winter"
          ]
        },
        "textures": {
          "Array.textureVariants": [
            "Texture.default",
            "Texture.shiny_default",
            "Texture.summer",
            "Texture.autumn",
            "Texture.winter"
          ]
        }
      },
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "geometry": "Array.geometryVariants[(query.skin_id==0)?0:(query.skin_id==1)?1:(query.skin_id==2)?2:(query.skin_id==3)?3:0]",
      "textures": [
        "Array.textureVariants[query.skin_id==0?query.property('pokeb:shiny')==true?1:0:(query.skin_id+1)]"
      ]
    }
  }
}
