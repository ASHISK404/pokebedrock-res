{
  "format_version": "1.10.0",
  "minecraft:attachable": {
    "description": {
      "identifier": "pokeb:backpack",
      "item": {
        "pokeb:backpack": "query.owner_identifier == 'minecraft:player'"
      },
      "materials": {
        "default": "armor",
        "enchanted": "armor_enchanted"
      },
      "textures": {
        "default": "textures/attachables/backpack",
        "enchanted": "textures/misc/enchanted_item_glint"
      },
      "geometry": {
        "default": "geometry.backpack"
      },
      "scripts": {
        "parent_setup": "variable.chest_layer_visible = 0.0;",
        "animate": ["offset", "walking"]
      },
      "animations": {
        "offset": "animation.armor.chestplate.offset",
        "walking": "animation.backpack.walking"
      },
      "render_controllers": ["controller.render.armor"]
    }
  }
}
