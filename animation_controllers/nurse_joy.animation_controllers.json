{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.nurse_joy": {
			"states": {
				"default": {
					"animations": ["idle"],
					"transitions": [
						{"side": "!q.relative_block_has_any_tag(0,0,1,'wood')"}
