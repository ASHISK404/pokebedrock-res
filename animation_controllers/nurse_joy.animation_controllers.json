					"blend_transition": 0.4,
					"blend_via_shortest_path": true
				},
				"side": {
					"animations": ["side"],
					"transitions": [
						{"default": "q.relative_block_has_any_tag(0,0,1,'wood')"}
					],
					"blend_transition": 0.4,
					"blend_via_shortest_path": true
				}
			}
		}
	}
}