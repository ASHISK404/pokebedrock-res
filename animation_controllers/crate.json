{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.crate": {
      "initial_state": "default",
      "states": {
        "default": {
          "animations": [
            {
              "rotate_0": "(query.variant == 0)"
            },
            {
              "rotate_90": "(query.variant == 1)"
            },
            {
              "rotate_180": "(query.variant == 2)"
            },
            {
              "rotate_270": "(query.variant == 3)"
            }
          ]
        }
      }
    }
  }
}
